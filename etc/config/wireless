
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path '10180000.wmac'
	option htmode 'HT20'
	option disabled '0'
	option txpower '20'
	option country '00'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option ssid 'hello@xt-sys'
	option network 'lan'
	option encryption 'psk-mixed'
	option key '12345678'

